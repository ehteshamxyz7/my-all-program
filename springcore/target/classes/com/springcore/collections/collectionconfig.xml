<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
                        http://www.springframework.org/schema/beans/spring-beans.xsd" >
                         
                          

	<bean name="Emp" class="com.springcore.collections.Employee">

	 <property name="name" value="Ehtesham Alam" /> 

		<property name="phones">
			<list>
				<value>98765432</value>
				<value>134556789</value>
				<value>838597785</value>

			</list>
		</property>


		<property name="addresses">
			<set>
				<value>Delhi</value>
				<value>Lucknow</value>
				<value>Mumbai</value>
			</set>
		</property>


		<property name="courses">
			<map>
				<entry key="java" value="2month" />
				<entry key="python" value="3month" />
				<entry key="phh" value="1month" />
			</map>
		</property>


	</bean>
</beans>
 		